{
  "expo": {
    "name": "BestOfWeb ",
    "description":
      "Best Web Meetups of Paris in a conference.",
    "slug": "bestofweb",
    "privacy": "public",
    "version": "1.0.4",
    "primaryColor": "#666",
    "icon": "./src/assets/icon-ios.png",
    "orientation": "portrait",
    "sdkVersion": "27.0.0",
    "scheme": "bestofweb",
    "assetBundlePatterns": ["**/*"],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "splash": {
      "image": "./src/assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF"
    },
    "android": {
      "package": "eventlama.apps.bestofweb",
      "icon": "./src/assets/icon-android.png",
      "versionCode": 5
    },
    "ios": {
      "bundleIdentifier": "bestofweb",
      "supportsTablet": true,
      "loadJSInBackgroundExperimental": true,
      "infoPlist": {
        "NSLocationAlwaysUsageDescription":
          "We don't track your location but some of the app modules have this feature though they are not used by the app.",
          "NSCameraUsageDescription": "Camera is needed to scan your QRcode."
      }
    }
  }
}
